<ul>
  <% @peep.each do |peep| %>
    <li class="peep" id="peep-<%= peep.id %>">
      "<%= peep.peep %>" - This was peeped at: <%= peep.readable_time %>
      <form action="/peep/edit/<%= peep.id %>" method="get">
        <input type="submit" value="Edit" />
      </form>
    </li>
  <% end %>
</ul>

<a href= '/peeping' > Post peep! </a>
